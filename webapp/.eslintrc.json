{
  "env": {
    "es6": true,
    "jest": true,
    "browser": true,
    "serviceworker": true
  },
  "extends": ["eslint:recommended", "plugin:vue/essential"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "warn"
  }
}
